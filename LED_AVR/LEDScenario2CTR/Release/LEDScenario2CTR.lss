                 	st  Y+, s12
                 
                 	ld  tmp, Y
                 	eor s23, tmp
                 	st  Y+, s23
                 
                 	ld  tmp, Y
                 	eor s33, tmp
                 	st  Y+, s33
                 
                 	ld  tmp, Y
                 	eor s03, tmp
                 	st  Y+, s03
                 
                 	ld  tmp, Y
