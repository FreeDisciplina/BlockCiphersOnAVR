                 .MACRO storeOutputSwitch
                 	ldi YH, high(SRAM_PTEXT)
                 	ldi YL, low(SRAM_PTEXT)
                 
                 	ld  tmp, Y
                 	eor s20, tmp
                 	st  Y+, s20
                 
                 	ld  tmp, Y
                 	eor s30, tmp
                 	st  Y+, s30
                 
                 	ld  tmp, Y
                 	eor s00, tmp
                 	st  Y+, s00
                 
                 	ld  tmp, Y
                 	eor s10, tmp
                 	st  Y+, s10
                 
