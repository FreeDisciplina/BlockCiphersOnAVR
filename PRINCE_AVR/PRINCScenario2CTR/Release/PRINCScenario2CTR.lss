                 	or   t3, t1
                 	eor  t0, s03
                 	eor  t1, s13
                 	eor  s01, t2
                 	eor  s11, t3
                 	movw s02, s01
                 	eor  s01, t0
                 	eor  s11, t1
                 	movw s03, s01
                 	or   s03, s00
                 	or   s13, s10
                 	and  t0, s03
                 	and  t1, s13
                 	eor  t0, t4
                 	eor  t1, t5
                 	movw s03, t0
                 	and  t0, s02
                 	and  t1, s12
                 	eor  s00, t0
                 	eor  s10, t1
                 
                 	; a = s30:s20 = r3:r2
                 	; b = s31:s21 = r7:r6
                 	; c = s32:s22 = r11:r10
                 	; d = s33:s23 = r15:r14
                 	movw t0, s20
                 	and  s20, s21
                 	and  s30, s31
                 	or   s20, s23
                 	or   s30, s33
                 	eor  s20, s22
                 	eor  s30, s32
                 	movw t2, s20
                 	com  s20
                 	com  s30
                 	or   t2, s21
                 	or   t3, s31
                 	eor  t2, s23
                 	eor  t3, s33
                 	or   s23, s20
                 	or   s33, s30
                 	movw t4, t2
                 	or   t2, t0
                 	or   t3, t1
                 	eor  t0, s23
                 	eor  t1, s33
                 	eor  s21, t2
                 	eor  s31, t3
                 	movw s22, s21
                 	eor  s21, t0
                 	eor  s31, t1
                 	movw s23, s21
                 	or   s23, s20
                 	or   s33, s30
                 	and  t0, s23
                 	and  t1, s33
                 	eor  t0, t4
                 	eor  t1, t5
                 	movw s23, t0
                 	and  t0, s22
                 	and  t1, s32
                 	eor  s20, t0
                 	eor  s30, t1
