                 	and t2, t5
                 	lsl  @2
                 	lsl  @2
                 	lsr  t2
                 	lsr  t2
                 	eor  @2, t2
                 
                 	lsr  @3
                 	bst  t3, 0
                 	bld  @3, 3
                 	bst  t3, 4
                 	bld  @3, 7
