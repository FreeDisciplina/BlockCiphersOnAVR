                     rol s7     
                     adc s6, rzero 
                 	lsl s6     
                     rol s7     
                     adc s6, rzero 
                 .ENDMACRO
                 
                 .MACRO forward_permutation
                     rotate16_left_row1
                     rotate16_left_row2
                     rotate16_left_row3
                 .ENDMACRO
                 
                 .MACRO invert_permutation
                     rotate16_right_row1
                     rotate16_right_row2
                     rotate16_right_row3
                 .ENDMACRO
                 
