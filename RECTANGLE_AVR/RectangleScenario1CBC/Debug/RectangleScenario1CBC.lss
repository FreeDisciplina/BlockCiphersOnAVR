                 .MACRO forward_key_update
                 	;forward_key_sbox
                 	;k0, k4, k8, k12
                 	mov  t0, k8
                 	eor  k8, k4
                 	com  k4
                 	mov  t1, k0
                 	and  k0, k4
                 	or   k4, k12
                 	eor  k4, t1
                 	eor  k12, t0
                 	eor  k0, k12
                 	and  k12, k4
                 	eor  k12, k8
                 	or   k8, k0
                 	eor  k8, k4
                 	eor  k4, t0
                 
                 	;w0: k3:k2:k1:k0
                 	;w1: k7:k6:k5:k4
                 	;w2: k11:k10:k9:k8
                 	;w3: k15:k14:k13:k12
                 
                 	;t3:t2:t1:t0  <- w3: k15:k14:k13:k12
                 	movw t0, k12
                 	movw t2, k14
                 	
